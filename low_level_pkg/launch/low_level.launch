<launch>
  <!-- Serial node for Arduino or similar device -->
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node" output="screen">
    <param name="port" value="/dev/ttyACM0" />
  </node>

  <!-- Odometry node -->
  <node pkg="low_level_pkg" type="odometry.py" name="odometry_node" output="screen" />

  <!-- IMU driver node -->
  <node pkg="low_level_pkg" type="imu_node.py" name="imu_node" output="screen" />

 <!-- Complementary filter node -->
  <node pkg="imu_complementary_filter" type="complementary_filter_node" name="complementary_filter_node" output="screen" />

  <!-- Launching another launch file -->
  <include file="$(find localization-stack)/launch/ekf_localization_local.launch" />

   <node pkg="tf" type="static_transform_publisher" name="static_tf_map_to_laser" 
          args="0 0 0 0 0 0 base_link laser 100" />

   <!-- <node pkg="tf" type="static_transform_publisher" name="static_tf_odom_to_map" args="0 0 0 0 0 0 odom map 100"/> -->
</launch>
